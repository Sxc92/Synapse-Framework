{
  "groups": [
    {
      "name": "synapse.security",
      "type": "com.indigo.security.config.SecurityProperties",
      "description": "安全配置属性",
      "sourceType": "com.indigo.security.config.SecurityProperties"
    }
  ],
  "properties": [
    {
      "name": "synapse.security.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用安全功能",
      "defaultValue": true,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.jwt.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用JWT认证",
      "defaultValue": true,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.jwt.secret",
      "type": "java.lang.String",
      "description": "JWT签名密钥",
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.jwt.expiration",
      "type": "java.lang.Long",
      "description": "JWT过期时间(秒)",
      "defaultValue": 86400,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.jwt.header-name",
      "type": "java.lang.String",
      "description": "JWT请求头名称",
      "defaultValue": "Authorization",
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.jwt.prefix",
      "type": "java.lang.String",
      "description": "JWT令牌前缀",
      "defaultValue": "Bearer ",
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用Sa-Token",
      "defaultValue": true,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.token-name",
      "type": "java.lang.String",
      "description": "Sa-Token令牌名称",
      "defaultValue": "satoken",
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.timeout",
      "type": "java.lang.Long",
      "description": "Sa-Token令牌超时时间(秒)",
      "defaultValue": 2592000,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.activity-timeout",
      "type": "java.lang.Long",
      "description": "Sa-Token活动超时时间(秒)",
      "defaultValue": -1,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.is-concurrent",
      "type": "java.lang.Boolean",
      "description": "是否允许同一账号并发登录",
      "defaultValue": true,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.is-share",
      "type": "java.lang.Boolean",
      "description": "是否在多个项目中共享Token",
      "defaultValue": false,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.is-log",
      "type": "java.lang.Boolean",
      "description": "是否输出操作日志",
      "defaultValue": false,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.is-read-cookie",
      "type": "java.lang.Boolean",
      "description": "是否从Cookie中读取Token",
      "defaultValue": false,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.is-read-header",
      "type": "java.lang.Boolean",
      "description": "是否从请求头中读取Token",
      "defaultValue": true,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.is-read-body",
      "type": "java.lang.Boolean",
      "description": "是否从请求体中读取Token",
      "defaultValue": false,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.is-write-header",
      "type": "java.lang.Boolean",
      "description": "是否在响应头中写入Token",
      "defaultValue": true,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.is-write-body",
      "type": "java.lang.Boolean",
      "description": "是否在响应体中写入Token",
      "defaultValue": false,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.is-write-cookie",
      "type": "java.lang.Boolean",
      "description": "是否在Cookie中写入Token",
      "defaultValue": false,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.token-prefix",
      "type": "java.lang.String",
      "description": "Token前缀",
      "defaultValue": "Bearer",
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.is-print",
      "type": "java.lang.Boolean",
      "description": "是否打印Sa-Token版本信息",
      "defaultValue": false,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.satoken.banner",
      "type": "java.lang.Boolean",
      "description": "是否显示Sa-Token banner",
      "defaultValue": false,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.oauth2.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用OAuth2.0",
      "defaultValue": false,
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.oauth2.client-id",
      "type": "java.lang.String",
      "description": "OAuth2客户端ID",
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.oauth2.client-secret",
      "type": "java.lang.String",
      "description": "OAuth2客户端密钥",
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.oauth2.authorization-server-url",
      "type": "java.lang.String",
      "description": "OAuth2授权服务器URL",
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.oauth2.token-server-url",
      "type": "java.lang.String",
      "description": "OAuth2令牌服务器URL",
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.oauth2.user-info-url",
      "type": "java.lang.String",
      "description": "OAuth2用户信息URL",
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.oauth2.redirect-uri",
      "type": "java.lang.String",
      "description": "OAuth2重定向URI",
      "sourceType": "com.indigo.security.config.SecurityProperties"
    },
    {
      "name": "synapse.security.oauth2.scope",
      "type": "java.lang.String",
      "description": "OAuth2授权范围",
      "defaultValue": "read",
      "sourceType": "com.indigo.security.config.SecurityProperties"
    }
  ]
} 